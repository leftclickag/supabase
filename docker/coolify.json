{
  "name": "Supabase Core (Lean)",
  "description": "Lean Supabase stack: Auth, DB, REST, Storage, Studio, Imgproxy",
  "version": "1.2.0",
  "services": {
    "default": {
      "env": {
        "POSTGRES_DB": {
          "default": "postgres"
        },
        "POSTGRES_HOST": {
          "default": "db"
        },
        "POSTGRES_PORT": {
          "default": "5432"
        },
        "POSTGRES_PASSWORD": {
          "generate": "password"
        },

        "JWT_SECRET": {
          "generate": "base64"
        },
        "JWT_EXPIRY": {
          "default": "3600"
        },

        "ANON_KEY": {
          "generate": "base64"
        },
        "SERVICE_ROLE_KEY": {
          "generate": "base64"
        },

        "PG_META_CRYPTO_KEY": {
          "generate": "password"
        },

        "SUPABASE_PUBLIC_URL": {
          "default": "http://localhost:3000"
        },

        "SITE_URL": {
          "default": "http://localhost:3000"
        },
        "API_EXTERNAL_URL": {
          "default": "http://localhost:3000"
        },

        "STUDIO_DEFAULT_ORGANIZATION": {
          "default": "Default Organization"
        },
        "STUDIO_DEFAULT_PROJECT": {
          "default": "Default Project"
        },

        "ENABLE_EMAIL_SIGNUP": {
          "default": "true"
        },
        "ENABLE_EMAIL_AUTOCONFIRM": {
          "default": "true"
        },

        "IMGPROXY_ENABLE_WEBP_DETECTION": {
          "default": "true"
        },

        "PGRST_DB_SCHEMAS": {
          "default": "public"
        }
      }
    }
  }
}
